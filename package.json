{
  "name": "devsy-components",
  "version": "1.0.0",
  "description": "UI components toolkit for devsy.io",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib dist",
    "build": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/devsy-components.js && NODE_ENV=production webpack src/index.js dist/devsy-components.min.js",
    "lint": "standard --verbose | ./node_modules/.bin/snazzy",
    "test": "NODE_ENV=test mocha",
    "test:watch": "NODE_ENV=test mocha --watch",
    "test:cov": "babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha",
    "prepublish": "npm run lint && npm run test && npm run clean && npm run build && npm run build:umd"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:devsy-io/devsy-components.git"
  },
  "keywords": [
    "devsy-components-keywords"
  ],
  "author": "devsy-components-author",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devsy-io/devsy-components.git/issues"
  },
  "homepage": "https://github.com/devsy-io/devsy-components.git",
  "devDependencies": {
    "babel": "^5.5.8",
    "babel-core": "^5.6.18",
    "babel-eslint": "^3.1.15",
    "babel-loader": "^5.1.4",
    "expect": "^1.6.0",
    "isparta": "^3.0.3",
    "mocha": "^2.2.5",
    "rimraf": "^2.3.4",
    "standard": "^7.1.2",
    "webpack": "^1.9.6",
    "webpack-dev-server": "^1.8.2"
  },
  "dependencies": {
    "invariant": "^2.0.0"
  },
  "standard": {
    "globals": [
      "it",
      "describe"
    ]
  }
}
